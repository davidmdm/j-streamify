{
  "name": "j-streamify",
  "version": "2.0.0",
  "description": "Used to stringify javascript objects to json strings as a stream",
  "main": "./src/index.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "nyc": "^15.0.1"
  },
  "scripts": {
    "test": "nyc mocha \"test/**/*.test.js\""
  },
  "keywords": [
    "json",
    "stream",
    "stringify"
  ],
  "author": "David Desmarais-Michaud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidmdm/j-streamify"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "mocha": {
    "timeout": 5000,
    "exit": true
  }
}
